    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-bunyan-logger": "^2.0.0",
    "koa-respond": "^1.0.1",
    "koa-router": "^7.4.0",
    "leaflet": "^1.3.3",
    "leaflet.markercluster": "^1.4.0",
    "marked": "^0.5.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "most": "^1.7.3",
    "nodemon": "^1.18.3",
    "prop-types": "^15.6.2",
    "randomcolor": "^0.5.3",
    "react": "^16.4.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.2",
    "react-leaflet": "1",
    "react-leaflet-markercluster": "^1.1.8",
    "react-router-dom": "^4.3.1",
    "tachyons": "^4.11.1"
  },
