    "@commitlint/config-conventional": "^7.0.1",
    "autoprefixer": "^9.1.2",
    "ava": "^1.0.0-beta.6",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
