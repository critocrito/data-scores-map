{
  "name": "data-scores-map",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "src/index.js",
  "config": {
    "srcDir": "src",
    "testDir": "test",
    "distDir": "dist",
    "configFiles": "{webpack,postccs}.config.js",
    "storiesDir": "stories"
  },
  "scripts": {
    "commitmsg":
      "! git rev-parse --abbrev-ref HEAD | grep -q master || validate-commit-msg",
    "precommit":
      "! git rev-parse --abbrev-ref HEAD | grep -q master || (lint-staged && npm run test)",
    "prepublishOnly": "npm run build",
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm run test -- -w",
    "coverage": "cross-env NODE_ENV=test nyc npm test",
    "flow-coverage": "flow-coverage-report --config ./.flowcoverage",
    "lint":
      "eslint $npm_package_config_configFiles $npm_package_config_storiesDir $npm_package_config_testDir $npm_package_config_srcDir",
    "lint-css": "stylelint src/**/*.css",
    "fix": "npm run lint -- --fix",
    "fix-css": "npm run lint-css -- --fix",
    "precompile": "npm run clean",
    "compile":
      "cross-env NODE_ENV=production webpack --mode production -r babel-register",
    "clean": "rimraf $npm_package_config_distDir",
    "build":
      "npm run lint && flow && npm run lint-css && npm run test && npm run compile",
    "start":
      "cross-env NODE_ENV=developemnt webpack-dev-server --mode development --open -r babel-register",
    "dev":
      "cross-env NODE_ENV=development nodemon --exec babel-node src/server.js | bunyan -o short",
    "prerelease": "npm run build",
    "release":
      "standard-version -i CHANGELOG.md -m \"chore(release): Bumped to version %s.\" --sign",
    "release-candidate": "npm run release -- --prerelease \"rc\"",
    "security-scan": "retire -p",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "Christo <christo@cryptodrunks.net>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-links": "^3.4.2",
    "@storybook/addons": "^3.4.2",
    "@storybook/cli": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "autoprefixer": "^8.3.0",
    "ava": "^0.25.0",
    "ava-jsverify": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.70.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-lint": "^0.1.0",
    "jsonlint": "^1.6.3",
    "jsverify": "^0.8.3",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "nyc": "^11.7.1",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "retire": "^1.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "standard-version": "^4.3.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "validate-commit-message": "^3.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-copy-plugin": "^0.0.4",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.1"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@sugarcube/plugin-elasticsearch": "^0.9.0",
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.1",
    "koa": "^2.5.1",
    "koa-bunyan-logger": "^2.0.0",
    "koa-respond": "^1.0.1",
    "koa-router": "^7.4.0",
    "nodemon": "^1.17.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "ava": {
    "sources": ["src/**/*.js"],
    "babel": "inherit",
    "require": ["babel-register"]
  }
}
