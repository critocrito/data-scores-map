    "flow-typed": "^2.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.1",
    "mobx-react-devtools": "^6.0.3",
    "nyc": "^12.0.2",
    "postcss-css-variables": "^0.9.0",
